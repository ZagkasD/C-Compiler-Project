program PassOrFail
{

	declare project, exams;
	
	

	function calculateGrade(in x, in y)
	{

		switchcase
			case(x/2<5 or y/2<5 )
		
			return (0);
			default return(1);

	}

	input(project);
	input(exam);

	if (calculateGrade(in project, in exam)=1)
		print(1);
	else
		print(0);
}.
